#!/bin/bash

echoerr() { printf "%s\n" "$*" >&2; }

echo Commandline:
echo
echo "$0" "$@"
echo
echo ---------------------------------------------------------------------------
echo Attachment:
if [ -f "$1" ]; then
    echo "File $1 exists."
    ls -la $1
else
    echo "File $1 does not exist."
fi
echo
echo ---------------------------------------------------------------------------
echo Environment:
env

if [ -n "$RESULT_FILE" ]; then
    env | jq -R 'split("=") | {(.[0]): .[1]}' | jq -s add > "$RESULT_FILE"
fi

echoerr "StdErr Output example"
