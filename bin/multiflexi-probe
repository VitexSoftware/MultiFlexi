#!/bin/bash

echoerr() { printf "%s\n" "$*" >&2; }

echo Commandline:
echo
echo $0 $@
echo
echo ---------------------------------------------------------------------------
echo Attachment:
if [ -f "$1" ]; then
    echo "File $1 exists."
    ls -la $1
else
    echo "File $1 does not exist."
fi
echo
echo ---------------------------------------------------------------------------
echo Environment:
env
echoerr "StdErr Output example"
